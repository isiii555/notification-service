{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MessagingProviders": {
    "Twilio": {
      "Enabled": true,
      "Priority": 1,
      "ApiKey": "twilioApiKey",
      "Channels": [ "SMS", "Email" ]
    },
    "AmazonSNS": {
      "Enabled": true,
      "Priority": 2,
      "ApiKey": "amazonSnsApiKey",
      "Channels": [ "SMS", "Push" ]
    }
  },
  "AllowedHosts": "*"
}
